{
    "env": {
        "NODE_ENV": "development"
    },
    "ignore": [
      "*.test.js",
      "logs/*"
    ],
    "ext": "js",
    "watch": [
      "*.js",
      "*.cjs",
      "commands/*.js",
      "services/*.js"
    ],
    "delay": "5000",
    "execMap": {
        "js": "node --trace-warnings"
    }
}
